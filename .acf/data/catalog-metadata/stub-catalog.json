{
  "methods": [
    {
      "method_id": "crawler_twitter_profile",
      "name": "Twitter Profile Crawler",
      "type": "crawler",
      "description": "Fetch Twitter profile data including posts, followers, and metadata",
      "inputs": {
        "username": {
          "type": "string",
          "required": true,
          "description": "Twitter username (without @)"
        }
      },
      "outputs": {
        "profile": {
          "type": "object",
          "schema": {
            "username": "string",
            "display_name": "string",
            "bio": "string",
            "followers_count": "integer",
            "following_count": "integer",
            "profile_picture": "string",
            "recent_posts": "array"
          }
        }
      },
      "tags": ["social_media", "twitter", "osint"]
    },
    {
      "method_id": "crawler_facebook_profile",
      "name": "Facebook Profile Crawler",
      "type": "crawler",
      "description": "Fetch Facebook profile data including posts and friends",
      "inputs": {
        "user_id": {
          "type": "string",
          "required": true,
          "description": "Facebook user ID"
        }
      },
      "outputs": {
        "profile": {
          "type": "object",
          "schema": {
            "user_id": "string",
            "name": "string",
            "about": "string",
            "friends_count": "integer",
            "profile_picture": "string",
            "recent_posts": "array"
          }
        }
      },
      "tags": ["social_media", "facebook", "osint"]
    },
    {
      "method_id": "crawler_linkedin_profile",
      "name": "LinkedIn Profile Crawler",
      "type": "crawler",
      "description": "Fetch LinkedIn professional profile data",
      "inputs": {
        "profile_url": {
          "type": "string",
          "required": true,
          "description": "LinkedIn profile URL"
        }
      },
      "outputs": {
        "profile": {
          "type": "object",
          "schema": {
            "full_name": "string",
            "headline": "string",
            "company": "string",
            "location": "string",
            "connections": "integer",
            "experience": "array",
            "skills": "array"
          }
        }
      },
      "tags": ["social_media", "linkedin", "professional"]
    },
    {
      "method_id": "ml_face_recognition",
      "name": "Face Recognition Model",
      "type": "ml_model",
      "description": "Extract 512-dimensional face embeddings from images",
      "inputs": {
        "images": {
          "type": "array",
          "required": true,
          "description": "Array of image URLs or base64-encoded images"
        }
      },
      "outputs": {
        "face_embeddings": {
          "type": "array",
          "schema": {
            "image_index": "integer",
            "face_id": "string",
            "embedding": "array[512]",
            "confidence": "float",
            "bbox": "object"
          }
        }
      },
      "tags": ["ml", "computer_vision", "face_recognition"]
    },
    {
      "method_id": "ml_sentiment_analysis",
      "name": "Sentiment Analysis Model",
      "type": "ml_model",
      "description": "Analyze sentiment of text content (positive/negative/neutral)",
      "inputs": {
        "texts": {
          "type": "array",
          "required": true,
          "description": "Array of text strings to analyze"
        }
      },
      "outputs": {
        "sentiment_scores": {
          "type": "array",
          "schema": {
            "text_index": "integer",
            "sentiment": "string",
            "positive_score": "float",
            "negative_score": "float",
            "neutral_score": "float"
          }
        }
      },
      "tags": ["ml", "nlp", "sentiment"]
    },
    {
      "method_id": "ml_ner",
      "name": "Named Entity Recognition",
      "type": "ml_model",
      "description": "Extract named entities (persons, organizations, locations) from text",
      "inputs": {
        "texts": {
          "type": "array",
          "required": true,
          "description": "Array of text strings to process"
        }
      },
      "outputs": {
        "entities": {
          "type": "array",
          "schema": {
            "text_index": "integer",
            "entity_text": "string",
            "entity_type": "string",
            "start_pos": "integer",
            "end_pos": "integer",
            "confidence": "float"
          }
        }
      },
      "tags": ["ml", "nlp", "ner"]
    },
    {
      "method_id": "breach_db_lookup",
      "name": "Breach Database Lookup",
      "type": "function",
      "description": "Search breach databases for leaked credentials",
      "inputs": {
        "email": {
          "type": "string",
          "required": true,
          "description": "Email address to search"
        }
      },
      "outputs": {
        "breaches": {
          "type": "array",
          "schema": {
            "breach_name": "string",
            "breach_date": "string",
            "data_classes": "array",
            "password_leaked": "boolean"
          }
        }
      },
      "tags": ["osint", "breach_data", "security"]
    }
  ],
  "ontology": {
    "entities": [
      {
        "entity_id": "social_media_profile",
        "name": "Social Media Profile",
        "description": "Generic social media profile entity",
        "schema": {
          "platform": {"type": "string", "required": true},
          "username": {"type": "string", "required": true},
          "display_name": {"type": "string"},
          "bio": {"type": "string"},
          "followers_count": {"type": "integer"},
          "following_count": {"type": "integer"},
          "profile_picture_url": {"type": "string"},
          "verified": {"type": "boolean"},
          "created_at": {"type": "timestamp"}
        }
      },
      {
        "entity_id": "breach_record",
        "name": "Data Breach Record",
        "description": "Leaked credential from data breach",
        "schema": {
          "email": {"type": "string", "required": true},
          "breach_name": {"type": "string", "required": true},
          "breach_date": {"type": "date"},
          "data_classes": {"type": "array"},
          "password_hash": {"type": "string"},
          "password_plaintext": {"type": "string"}
        }
      },
      {
        "entity_id": "person",
        "name": "Person Entity",
        "description": "Individual person with linked identities",
        "schema": {
          "full_name": {"type": "string", "required": true},
          "aliases": {"type": "array"},
          "date_of_birth": {"type": "date"},
          "location": {"type": "string"},
          "social_profiles": {"type": "array"},
          "email_addresses": {"type": "array"},
          "phone_numbers": {"type": "array"}
        }
      }
    ]
  }
}
